<AnchorPane prefHeight="553.0" prefWidth="925.0" styleClass="backgroundColor" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo13.ImageController">
    <children>
