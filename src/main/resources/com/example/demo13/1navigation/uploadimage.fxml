                <FontAwesomeIconView glyphName="UPLOAD" layoutX="305.0" layoutY="207.0" size="50" wrappingWidth="19.0">
                    <font>
                        <Font size="64.0" />
                    </font>
                </FontAwesomeIconView>
                <ImageView fx:id="imageView" fitHeight="181.0" fitWidth="200.0" layoutX="232.0" layoutY="270.0" pickOnBounds="true" preserveRatio="true" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
