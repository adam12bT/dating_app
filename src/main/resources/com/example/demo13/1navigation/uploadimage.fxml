            <children>
                <Label layoutX="197.0" layoutY="54.0" prefHeight="38.0" prefWidth="270.0" style="-fx-font-style: normal;" text="Add your Favorite Photos!">
                    <font>
                        <Font name="Berlin Sans FB" size="24.0" />
                    </font>
                </Label>
                <Button fx:id="uploadButton" layoutX="250.0" layoutY="222.0" mnemonicParsing="false" onAction="#onUpload" prefHeight="26.0" prefWidth="164.0" style="-fx-background-color: #000000; -fx-background-radius: 60;" text="Upload Image" textFill="#fffbfb">
